// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @id @default(autoincrement())
  firstName String 
  lastName      String
  faceDescriptor Int[]
  tfaToken String
  role String
  dateCreated String
  enabled Boolean @default(false)
}
